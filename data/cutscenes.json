{
    "Ellipse": {
        "start": {
            "player": {"pos": [64, 144]},
            "ellipse": {"pos": [256, 144], "room": 0}
        },
        "time_commands": {
            "0": [
                "player controllable",
                "player jump not",
                "ellipse face left",
                "text create tutorialText",
                "text tutorialText move 192 20",
                "text tutorialText change Use W and D or LEFT and RIGHT to move",
                "text tutorialText display",
                "run testMoved"
            ]
        },
        "conditionals": {
            "testMoved": "player x >= 120"
        },
        "cond_commands":{
            "testMoved": [
                "player uncontrollable",
                "player face right",
                "text create ellipse",
                "text ellipse change I am here to warn you...",
                "text ellipse move 264 120",
                "text ellipse display",
                "delay 165 step2"
            ],
            "step2": [
                "text ellipse hide",
                "delay 30 step3"
            ],
            "step3": [
                "text ellipse change The Belloq are coming...",
                "text ellipse display",
                "delay 165 step4"
            ],
            "step4": [
                "text ellipse hide",
                "ellipse walk right",
                "player controllable",
                "player jump can"
            ]
        }
    },

    "Corlen": {
        "backgroundAnim": {"path": "res/cutscenes/corlen's_house.png", "room": 1},
        "start": {
            "player": {"pos": [0, 192]},
            "ellipse": {"pos": [384, 192], "room": 1},
            "corlen": {"pos": [210, 192], "room": 0}
        },

        "time_commands": {
            "0": [
                "player jump not",
                "player controllable",
                "run corlenNotice"
            ]
        },
        "conditionals": {
            "corlenNotice": "player x > 90",
            "corlenStop": "corlen room == 1 and corlen x > 110",
            "playerMovingOn": "room == 1 and player x >= 20",
            "ellipseStop": "ellipse x < 320",
            "corlenBehindPlayer": "corlen x < 4",
            "corlenGoesBack": "corlen x > 110",
            "nextRoom": "room == 1"
        },
        "cond_commands": {
            "nextRoom": ["text pressEnterToSkip hide"],

            "corlenNotice": [
                "text create corlen",
                "text corlen color 252 227 3",
                "text corlen change !",
                "text corlen move 218 170",
                "text corlen display",

                "corlen face left",
                
                "text create pressEnterToSkip",
                "text pressEnterToSkip change Press enter to skip forward through text!",
                "text pressEnterToSkip move 192 50",
                "text pressEnterToSkip display",

                "run nextRoom",
                
                "player uncontrollable",
                "delay 60 whoRU"
            ], "whoRU": [
                "text corlen change Who are you?",
                "delay 120 noResponse..."
            ], "noResponse...": [
                "text corlen hide",
                "delay 60 turnRound"
            ], "turnRound": [
                "corlen face right",
                "delay 60 movingOn"
            ], "movingOn": [
                "corlen face left",
                "text corlen display",
                "text corlen change Not a big talker, huh?",
                "delay 160 introducing"
            ], "introducing": [
                "text corlen change Well, uh, I'm Corlen.",
                "text corlen display",
                "delay 150 randomHallway"
            ], "randomHallway": [
                "text corlen move 218 155",
                "text corlen change And this is a random\nhallway in the middle of nowhere.",
                "delay 180 delayyy"
            ], "delayyy": [
                "text corlen hide",
                "delay 80 newRoundHere"
            ], "newRoundHere": [
                "text corlen display",
                "text corlen move 218 170",
                "text corlen change You're new around here, aren't you?",
                "delay 160 newbieFight"
            ], "newbieFight": [
                "text corlen change Only a newbie would fight the Belloq like that.",
                "delay 190 saysNothing"
            ], "saysNothing": [
                "text corlen hide",
                "delay 90 ropes"
            ], "ropes": [
                "text corlen change Oh well, guess I'll show you the ropes.",
                "text corlen display",
                "delay 160 follow"
            ], "follow": [
                "text corlen change Follow me.",
                "delay 120 allowFollow"
            ], "allowFollow": [
                "text corlen hide",
                "corlen walk right",
                "player controllable",
                "run corlenStop",
                "run playerMovingOn"
            ],

            "corlenStop": [
                "corlen face left",
                "corlen walk still"
            ],
            
            "playerMovingOn": [
                "player uncontrollable",
                "text corlen move 118 170",
                "text corlen change This is my humble abode.",
                "text corlen display",
                "delay 130 notMuch"
            ], "notMuch": [
                "text corlen move 118 155",
                "text corlen change It's not much, but\nit's enough for me.",
                "delay 140 silence."
            ], "silence.": [
                "text corlen hide",
                "delay 60 thinking"
            ], "thinking": [
                "text corlen display",
                "text corlen change You may be thinking, \n\"What is this place?\"",
                "delay 170 assuming"
            ], "assuming": [
                "text corlen change I'm assuming you're thinking that\nbecause you haven't said a word yet.",
                "delay 190 goodQuestion"
            ], "goodQuestion": [
                "text corlen move 118 170",
                "text corlen change That's a good question.",
                "delay 130 dunnoMan"
            ], "dunnoMan": [
                "text corlen change But I don't know either.",
                "delay 130 noOneElse"
            ], "noOneElse": [
                "text corlen move 118 155",
                "text corlen change There's no one else around\nbesides you and me,",
                "delay 140 allTheRest"
            ], "allTheRest": [
                "text corlen change and those crazy monsters\ncalled the Belloq.",
                "delay 70 ellipseAppears"
            ], "ellipseAppears": [
                "ellipse walk left",
                "run ellipseStop",
                "delay 140 AHHHHH"
            ], 
            
            "ellipseStop": ["ellipse walk still"],

            "AHHHHH": [
                "text corlen move 118 170",
                "text corlen change AHHHHH A BELLOQ!",
                "corlen face right",
                "delay 130 careful"
            ], "careful": [
                "text corlen move 118 155",
                "text corlen change Be careful, Belloq are very dangerous.\nWe should get out of here quickly!",
                "delay 190 ellipseTalked!"
            ], "ellipseTalked!": [
                "text create ellipse",
                "text ellipse change ...",
                "text ellipse color 255 255 255",
                "text ellipse move 328 170",
                "text ellipse display",
                "text corlen hide",
                "delay 160 itTalked"
            ], "itTalked": [
                "text ellipse hide",
                "text corlen display",
                "text corlen move 118 170",
                "text corlen change It talked!",
                "delay 90 corlenMove"
            ], "corlenMove": [
                "text corlen hide",
                "corlen walk left",
                "run corlenBehindPlayer"
            ],

            "corlenBehindPlayer": [
                "corlen walk still",
                "corlen face right",
                "delay 20 moreDots"
            ],

            "moreDots": [
                "text ellipse display",
                "delay 160 hmm"
            ], "hmm": [
                "text ellipse hide",
                "text corlen display",
                "text corlen move 20 170",
                "text corlen change Hmm...",
                "delay 140 moveRight"
            ], "moveRight": [
                "text corlen hide",
                "corlen walk right",
                "run corlenGoesBack"
            ],
            
            "corlenGoesBack": [
                "corlen walk still",
                "text corlen move 118 170",
                "text corlen display",
                "text corlen change Maybe you aren't a Belloq after all.",
                "delay 140 oneOfUs"
            ], "oneOfUs": [
                "corlen face left",
                "text corlen change If it can talk, maybe it's one of us!",
                "delay 150 ellipseSayEllipse"
            ], "ellipseSayEllipse": [
                "corlen face right",
                "text ellipse display",
                "text corlen hide",
                "delay 160 namingEllipse"
            ], "namingEllipse": [
                "text ellipse hide",
                "text corlen display",
                "text corlen move 118 155",
                "text corlen change I think I'm going to call\nyou... Ellipse!",
                "delay 140 greatIdeaRight?"
            ], "greatIdeaRight?": [
                "text corlen move 118 170",
                "text corlen change What a great nickname, right Ellipse?",
                "delay 140 ellipseResponds"
            ], "ellipseResponds": [
                "text corlen hide",
                "text ellipse display",
                "delay 160 youKnowWhat?"
            ], "youKnowWhat?": [
                "text ellipse hide",
                "text corlen change You know what?",
                "text corlen display",
                "delay 20 switchDirs"
            ], "switchDirs": [
                "corlen face left",
                "delay 20 faceyRight"
            ], "faceyRight": [
                "corlen face right",
                "delay 40 makeGoodTeam"
            ], "makeGoodTeam": [
                "text corlen change We could make a good team,",
                "delay 130 names"
            ], "names": [
                "text corlen change Corlen, Ellipse, and a weird human!",
                "delay 120 faceLeft"
            ], "faceLeft": [
                "corlen face left",
                "delay 30 sickOfHouse"
            ], "sickOfHouse": [
                "text corlen move 118 155",
                "text corlen change And I'm sick of staying\nin my house all day.",
                "delay 160 goOnAdventure"
            ], "goOnAdventure": [
                "corlen face right",
                "text corlen move 118 170",
                "text corlen change I want to go on an adventure!",
                "delay 140 comparisons"
            ], "comparisons": [
                "corlen face left",
                "text corlen move 118 155",
                "text corlen change And if a tiny human can survive\na Belloq encounter, I can too!",
                "delay 160 dootdootdoot"
            ], "dootdootdoot": [
                "text ellipse display",
                "text corlen hide",
                "corlen face right",
                "delay 160 puttingWordsInMouth"
            ], "puttingWordsInMouth": [
                "text corlen display",
                "text ellipse hide",
                "text move 118 170",
                "text corlen change See, Ellipse approves!",
                "corlen face left",
                "delay 130 mooooreDoots"
            ], "mooooreDoots": [
                "text ellipse display",
                "text corlen hide",
                "corlen face right",
                "delay 130 leadDahWay"
            ], "leadDahWay": [
                "corlen face left",
                "text corlen display",
                "text ellipse hide",
                "text corlen change Alright, blue human, lead the way!",
                "delay 150 finish"
            ], "finish": [
                "text corlen hide",
                "corlen face right",
                "player controllable",
                "player jump can"
            ]
        }
    },

    "Ruins": {
