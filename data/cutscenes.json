                "player jump not",
                "player controlled",
                "run corlenNotice"
            ]
        },
        "conditionals": {
            "corlenNotice": "player x > 90"
        },
        "cond_commands": {
            "corlenNotice": [
                "text change !",
                "text move 218 170",
                "text display",
                "corlen face left",
